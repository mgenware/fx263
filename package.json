{
  "name": "mingru",
  "version": "0.8.0",
  "description": "SQL Generator for dd-models",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "author": "Mgen (https://www.mgenware.com)",
  "homepage": "https://github.com/mgenware/mingru",
  "bugs": "https://github.com/mgenware/mingru/issues",
  "keywords": [
    "mingru"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mgenware/mingru.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "yarn run clean && yarn run pi && yarn run compile && yarn run lint && yarn run t",
    "t": "jest",
    "build": "yarn test",
    "compile": "tsc -b tests",
    "dev": "yarn run clean && yarn run pi && tsc -b tests -w",
    "prepublishOnly": "yarn run test",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "clean": "del-cli dist && del-cli dist_tests",
    "pi": "yarn add dd-models -P",
    "dev_no_pi": "yarn run clean && tsc -b tests -w"
  },
  "engines": {
    "node": ">=8.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ]
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.4",
    "del-cli": "^2.0.0",
    "jest": "^24.8.0",
    "tempy": "^0.3.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "del": "^4.1.1",
    "m-fs": "^2.3.0",
    "sql-escape-string": "^1.1.0",
    "throw-if-arg-empty": "^2.0.1",
    "to-type-string": "^1.0.0"
  },
  "peerDependencies": {
    "dd-models": "^0.15.0"
  }
}
