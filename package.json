{
  "name": "mingru",
  "version": "0.26.0",
  "description": "SQL Generator for mingru-models",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "author": "Mgen (https://www.mgenware.com)",
  "homepage": "https://github.com/mgenware/mingru",
  "bugs": "https://github.com/mgenware/mingru/issues",
  "keywords": [
    "mingru"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mgenware/mingru.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "yarn run clean && yarn run compile && yarn run lint && yarn run t",
    "t": "mocha --require source-map-support/register dist_tests/**/*.test.js",
    "build": "yarn test",
    "compile": "tsc -b tests",
    "dev": "yarn run clean && tsc -b tests -w",
    "prepublishOnly": "yarn run test",
    "lint": "eslint --max-warnings 0 --ext .ts src/ tests/",
    "clean": "del-cli dist && del-cli dist_tests"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/dist/**/*.js",
    "/dist/**/*.ts",
    "/dist/**/*.map"
  ],
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "it-throws": "^3.2.1",
    "mingru-models": "^0.33.2",
    "mocha": "^7.1.0",
    "source-map-support": "^0.5.16",
    "tempy": "^0.5.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "del": "^5.1.0",
    "m-fs": "^2.3.4",
    "sql-escape-string": "^1.1.0",
    "throw-if-arg-empty": "^2.2.1",
    "to-type-string": "^1.0.2"
  }
}
